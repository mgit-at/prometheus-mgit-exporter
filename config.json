{
  "listen": ":9328",
  "certfile": {
    "enable": false,
    "globs": [
      "/etc/ssl/**/*.pem",
      "/etc/ssl/**/*.crt"
    ],
    "exclude_system": true
  },
  "mcelog": {
    "enable": false,
    "path": "/var/log/mcelog"
  },
  "fstab": {
    "enable": true
  },
  "rasdaemon": {
    "enable": true
  },
  "elk": {
    "enable": true,
    "duration": "170h",
    "node": "m3-hot"
  },
  "exec": {
    "enable": false,
    "scripts": {}
  }
}
